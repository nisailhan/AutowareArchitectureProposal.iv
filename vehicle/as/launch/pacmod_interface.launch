<?xml version="1.0"?>
<launch>

  <arg name="pacmod_param_path" default="$(find as)/config/pacmod_param.yaml"/>   
  <arg name="pacmod_extra_param_path" default="$(find as)/config/pacmod_extra_param.yaml"/>


  <!-- pacmod interface -->
  <node pkg="as" type="pacmod_interface" name="pacmod_interface" output="screen">
    <rosparam command="load" file="$(arg pacmod_param_path)"/>
  </node>

  <!-- pacmod additional parameter changer -->
  <node pkg="as" type="pacmod_dynamic_parameter_changer" name="pacmod_dynamic_parameter_changer" output="screen">
    <rosparam command="load" file="$(arg pacmod_extra_param_path)" />
    <remap from="~output/can" to="/pacmod/can_rx"/>
    <remap from="~input/steer_rpt" to="/pacmod/parsed_tx/steer_rpt"/>
  </node>

</launch>
