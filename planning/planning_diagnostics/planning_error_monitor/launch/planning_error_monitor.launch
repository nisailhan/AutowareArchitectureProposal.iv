<launch>
    <arg name="input/trajectory" default="/planning/scenario_planning/trajectory" />

    <node name="planning_error_monitor" type="planning_error_monitor" pkg="planning_error_monitor" output="screen">
        <remap from="~input/trajectory" to="$(arg input/trajectory)" />
        <param name="error_interval" value="100.0" />
        <param name="error_curvature" value="2.0" />
    </node>
</launch>
